from airflow.decorators import dag, task
import datetime

@dag(
    start_date=datetime(2025, 1, 1), 
    schedule_interval='@daily',
    catchup=False
)
def taskflow_dag():
    #@task декоратор нь Python функцийг Airflow task болгон хувиргадаг.
    @task
    def fetch_data():
        return ["data1", "data2"]  # Жишээ өгөгдөл
    
    @task
    def process_data(data):
        print(f"Processing: {data}")
    
    #fetch_data-ийн гаралт (data) шууд process_data-д дамжигдана.
    data = fetch_data()
    process_data(data)

dag = taskflow_dag()